<?xml version="1.0" encoding="utf-8"?>
<extend_features>
    <app_feature args="StringList:type" name="com.android.phone.simsettings.disable_apn_preferences">
        <StringList args="bearer" name="type"/>
    </app_feature>
    <app_feature name="com.android.phone.simsettings.customize_vowifi_switch_title_dynamic" args="String:de_vdf_vowifi_enable_title" />
    <app_feature name="com.android.phone.simsettings.vdf_vowifi_dialog_dynamic" />
    <app_feature name="com.android.phone.simsettings.customize_vowifi_dialog_dynamic" args="StringList:value">
        <StringList name="value" args="key_title;wifi_calling_vodafone_title"/>
        <StringList name="value" args="key_message;wifi_calling_vodafone_message"/>
        <StringList name="value" args="key_positive_button;wifi_calling_positive_button"/>
        <StringList name="value" args="key_negative_button;wifi_calling_negative_button"/>
        <StringList name="value" args="key_dialog_type;not_ask_dialog"/>
    </app_feature>
    <app_feature name="com.android.phone.simsettings.hide_5G_switch_in_develop" />
    <app_feature name="com.android.phone.network_list_visible_dynamic" args="String:show" />
    <app_feature name="com.android.phone.network_list_visible_5G_dynamic" args="String:show" />
    <app_feature args="StringList:type" name="com.android.phone.customize_network_name_list_5g_dynamic">
        <StringList args="oplus_5g_4g_3g_2g_auto" name="type"/>
        <StringList args="oplus_4g_3g_2g_auto" name="type"/>
    </app_feature>
    <app_feature args="StringList:type" name="com.android.phone.customize_network_value_list_5g_dynamic">
        <StringList args="33" name="type"/>
        <StringList args="9" name="type"/>
    </app_feature>
    <app_feature args="StringList:type" name="com.android.phone.customize_network_name_list_dynamic">
        <StringList args="oplus_4g_3g_2g_auto" name="type"/>
    </app_feature>
    <app_feature args="StringList:type" name="com.android.phone.customize_network_value_list_dynamic">
        <StringList args="9" name="type"/>
    </app_feature>
</extend_features>