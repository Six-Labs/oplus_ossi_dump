<oplus-config>
    <!-- OPLUS_FEATURE_DIRAC -->
    <oplus-feature name="oplus.software.audio.audioeffect_support"/>
    <oplus-feature name="oplus.software.audio.dirac_support"/>
    <oplus-feature name="oplus.software.audio.dirac_v2_support"/>
    <!-- Huade.zhou@MULTIMEDIA.AUDIOSERVER.EFFECT, 2020/11/03 add for enable dirac bt effect -->
    <oplus-feature name="oplus.software.audio.dirac.a2dp.support"/>
    <!-- MaYou@MULTIMEDIA.AUDIOSERVER.EFFECT, 2021/07/16, add for dirac black list feature -->
    <oplus-feature name="oplus.software.audio.dirac.blacklist.support"/>
    <!-- OPLUS_FEATURE_DIRAC END -->
    <!-- OPLUS_FEATURE_SPEAKER_REVERSE_CHANNEL -->
    <!-- YanWeibo@MULTIMEDIA.AUDIOSERVER.FRAMEWORK,2021/08/02, Add for reverse speaker channel,dual speaker -->
    <oplus-feature name="oplus.software.audio.dualspk_support"/>
    <!-- OPLUS_FEATURE_SPEAKER_REVERSE_CHANNEL END -->
</oplus-config>
